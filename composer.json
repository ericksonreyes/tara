{
  "name": "ericksonreyes/tara",
  "description": "Tara Office Suite",
  "license": "Propriety",
  "type": "library",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "fzaninotto/faker": "1.9.2",
    "behat/behat": "v3.8.1",
    "phpspec/phpspec": "7.0.1",
    "friends-of-phpspec/phpspec-code-coverage": "6.1.0",
    "phpunit/phpunit": "9.5.19",
    "sebastian/phpcpd": "6.0.3",
    "phing/phing": "2.16.4",
    "mockery/mockery": "1.4.4",
    "php-coveralls/php-coveralls": "2.4.3",
    "php-parallel-lint/php-var-dump-check": "^0.5.0",
    "php-parallel-lint/php-console-color": "v1.0",
    "php-parallel-lint/php-parallel-lint": "1.3.2",
    "phpmd/phpmd": "2.10.2",
    "povils/phpmnd": "2.4.0",
    "squizlabs/php_codesniffer": "3.6.2"
  },
  "autoload": {
    "psr-4": {
      "Tara\\": "src/Tara"
    },
    "psr-0": {
      "Tara\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "spec\\": "tests/spec/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "bin-dir": "bin",
    "platform-check": false
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
